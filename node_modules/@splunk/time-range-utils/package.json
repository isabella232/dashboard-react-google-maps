{
    "name": "@splunk/time-range-utils",
    "version": "2.6.1",
    "license": "SEE LICENSE IN LICENSE.md",
    "author": "Splunk",
    "files": [
        "/presets.js",
        "/time.js",
        "/timeParser.js",
        "/timeUnits.js"
    ],
    "scripts": {
        "build": "yarn babel src -d . --ignore \"**/tests/*.unit.js,**/docs/\"",
        "lint": "yarn root:eslint src --ext \".js\"",
        "lint:fix": "yarn root:eslint src --ext \".js\" --fix",
        "dev": "babel src -d . --watch --ignore \"**/tests/*.unit.js\"",
        "test": "yarn test:unit",
        "test:unit": "jest",
        "test:unit:ci": "yarn test:unit --ci --coverage",
        "test:unit:watch": "yarn test:unit --watch",
        "docs:gen": "yarn node ./build-jsdoc-markdown.js",
        "docs": "yarn docs:gen && NODE_ENV=production webpack --config webpack.docs.config.js",
        "docs:start": "yarn docs:gen && webpack-dev-server --config webpack.docs.config.js --watch",
        "docs:publish": "cicd-publish-docs"
    },
    "dependencies": {
        "@babel/runtime": "^7.8.4",
        "@splunk/moment": "^0.6.0",
        "@splunk/splunk-utils": "^2.0.0",
        "@splunk/ui-utils": "^1.2.1",
        "lodash": "^4.17.19"
    },
    "devDependencies": {}
}
