{
  "name": "@splunk/dashboard-inputs",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Splunk",
  "version": "23.1.0",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.js --bail",
    "build-storybook": "build-storybook -c .storybook -s ../../fonts -o .storybook-bundle",
    "build:ci": "yarn build && yarn docs",
    "clean": "rm -rf ./build && rm -rf ./test-reports",
    "dev": "NODE_ENV=development webpack --config ./webpack.config.js --watch",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:ci": "eslint src --ext \".js,.jsx\" -f junit -o test-reports/lint-results.xml",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "lint": "yarn run eslint",
    "lint:ci": "yarn run eslint:ci",
    "storybook": "start-storybook -p 6006 -c .storybook -s ../../fonts",
    "test": "yarn run test:unit",
    "test:ci": "yarn run test:unit:ci",
    "test:unit": "jest src/.* --passWithNoTests",
    "test:unit:ci": "yarn run test:unit --passWithNoTests",
    "test:unit:watch": "yarn run test:unit --watchAll",
    "test:visual": "yarn build-storybook --quiet && yarn loki test --reactUri file:./.storybook-bundle",
    "test:visual:update": "yarn loki approve",
    "test:visual:ci": "yarn test:visual --requireReference --verboseRenderer"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.12.5",
    "@splunk/dashboard-icons": "^23.1.0",
    "@splunk/dashboard-telemetry": "^23.1.0",
    "@splunk/dashboard-ui": "^23.1.0",
    "@splunk/dashboard-utils": "^23.1.0",
    "@splunk/moment": "^0.6.0",
    "@splunk/react-icons": "^3.1.0",
    "@splunk/react-time-range": "^7.0.3",
    "@splunk/react-ui": "^4.1.0",
    "@splunk/themes": "^0.8.0",
    "@splunk/time-range-utils": "^2.6.1",
    "@splunk/ui-utils": "^1.2.1",
    "@splunk/visualization-encoding": "^23.3.0",
    "@splunk/visualization-encoding-parsers": "^23.3.0",
    "lodash": "^4.17.4",
    "memoize-one": "^5.2.1",
    "prop-types": "^15.7.2",
    "react-sortable-hoc": "^1.11.0"
  },
  "peerDependencies": {
    "@splunk/dashboard-context": "^23.1.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@splunk/dashboard-build-tools": "^23.1.0",
    "@splunk/dashboard-context": "^23.1.0",
    "@splunk/react-time-range": "^7.0.3",
    "@splunk/themes": "^0.8.0",
    "@storybook/react": "^6.1.11",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^13.1.9",
    "babel-jest": "^25.1.0",
    "enzyme": "^3.10.0",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "jest": "^26.0.0",
    "jest-image-snapshot": "^4.0.0",
    "jest-styled-components": "^7.0.3",
    "loki": "^0.28.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-test-renderer": "16.14.0",
    "styled-components": "^5.2.1",
    "testing-library-selector": "^0.1.3",
    "ts-jest": "^26.0.0",
    "ts-loader": "^8.0.1",
    "webpack": "^4.29.6",
    "webpack-merge": "^4.2.1"
  }
}
