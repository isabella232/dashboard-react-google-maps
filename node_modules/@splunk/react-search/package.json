{
  "name": "@splunk/react-search",
  "version": "3.0.0",
  "description": "Splunk search component in React",
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "components",
    "utils",
    "themes.js"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack --bail",
    "clean": "git clean -xdf",
    "docs": "NODE_ENV=production webpack --config webpack.docs.config.js",
    "docs:start": "webpack-dev-server --config webpack.docs.config.js --watch",
    "eslint": "eslint src --ext \".js,.jsx\"",
    "eslint:ci": "eslint src --ext \".js,.jsx\" -f junit -o test-reports/lint-results.xml",
    "eslint:fix": "eslint src --ext \".js, .jsx\" --fix",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:ci": "yarn run eslint:ci && yarn run stylelint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "NODE_ENV=development webpack --watch",
    "start:demo": "webpack-dev-server --config demo/webpack.standalone.config.js",
    "stylelint": "stylelint \"src/**/*.{js,jsx}\" --config stylelint.config.js",
    "test": "jest --reporters=default",
    "test:ci": "jest --ci",
    "test:watch": "jest --reporters=default --watch",
    "test:functional:ci": "splunk-wdio-functional-test-runner functional.ci.conf.js",
    "test:functional": "splunk-wdio-functional-test-runner functional.local.conf.js",
    "publish:ci": "cicd-publish-package && cicd-publish-docs"
  },
  "dependencies": {
    "@splunk/react-time-range": "^7.0.0",
    "@splunk/splunk-utils": "^2.0.0",
    "@splunk/react-icons": "^3.0.1",
    "@splunk/react-ui": "^4.0.0",
    "@splunk/themes": "^0.7.0",
    "@splunk/ui-utils": "^1.2.1",
    "ace-builds": "^1.2.8",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/polyfill": "^7.0.0",
    "@splunk/babel-preset": "^3.0.0",
    "@splunk/build-tools-wc": "^0.0.1",
    "@splunk/cicd-tools": "^0.4.0",
    "@splunk/docs-gen": "^0.11.0",
    "@splunk/eslint-config-wc": "^0.0.1",
    "@splunk/stylelint-config": "^4.0.0",
    "@splunk/wc-docs": "^0.0.1",
    "@splunk/wdio-functional-test-runner": "^7.0.0",
    "@splunk/webpack-configs": "^5.0.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^4.1.4",
    "chai": "^4.2.0",
    "docgen-loader": "^1.3.4",
    "jest": "^24.8.0",
    "jest-junit": "^10.0.0",
    "lodash": "^4.17.11",
    "raf": "^3.4.0",
    "react": "^16.8.0",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.8.0",
    "styled-components": "^5.1.1",
    "stylelint": "^10.0.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.1.4"
  },
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8",
    "styled-components": "^5.1"
  },
  "engines": {
    "node": ">=6"
  }
}
