{
    "name": "@splunk/react-time-range",
    "version": "7.0.3",
    "description": "A Splunk time range picker built with React.",
    "license": "SEE LICENSE IN LICENSE.md",
    "author": "Splunk",
    "main": "Dropdown",
    "files": [
        "Dialog/*.js",
        "Dialog/*.d.ts",
        "Dropdown/*.js",
        "Dropdown/*.d.ts",
        "TimeAcceleratorPopup/*.js",
        "TimeAcceleratorPopup/*.d.ts",
        "SplunkwebConnector"
    ],
    "scripts": {
        "lint": "yarn root:eslint src --ext \".ts,.tsx\" && yarn root:tsc --noEmit",
        "lint:fix": "yarn root:eslint src --ext \".ts,.tsx\" --fix",
        "build": "yarn build:types && yarn babel src --extensions \".ts,.tsx,.js,.jsx\" --ignore \"**/tests/*,**/docs/*\" -d .",
        "build:types": "yarn root:tsc --project tsconfig.json -d --emitDeclarationOnly --outDir ./",
        "dev": "yarn build --watch",
        "test": "yarn test:unit",
        "test:unit": "jest",
        "test:unit:ci": "yarn test:unit --ci --coverage",
        "test:unit:watch": "yarn test:unit --watch",
        "test:visual": "build-storybook --quiet && loki --reactUri file:./storybook-static",
        "test:visual:update": "build-storybook --quiet && loki update --reactUri file:./storybook-static",
        "test:visual:ci": "build-storybook --quiet && loki --requireReference --reactUri file:./storybook-static",
        "test:visual:setup": "loki update --verboseRenderer",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "docs": "yarn docs:gen && NODE_ENV=production webpack --config webpack.docs.config.js",
        "docs:gen": "document-react-props src docs",
        "docs:start": "yarn docs:gen && webpack-dev-server --host 0.0.0.0 --config webpack.docs.config.js",
        "docs:publish": "cicd-publish-docs"
    },
    "dependencies": {
        "@babel/runtime": "^7.14.0",
        "@splunk/moment": "^0.6.0",
        "@splunk/react-icons": "^3.0.1",
        "@splunk/react-ui": "^4.0.3",
        "@splunk/splunk-utils": "^2.0.0",
        "@splunk/themes": "^0.7.0",
        "@splunk/time-range-utils": "^2.6.1",
        "@splunk/ui-utils": "^1.1.2",
        "lodash": "^4.17.19",
        "prop-types": "^15.6.0"
    },
    "peerDependencies": {
        "react": "^16.10.0",
        "react-dom": "^16.10.0",
        "styled-components": "^5.1"
    },
    "devDependencies": {}
}
